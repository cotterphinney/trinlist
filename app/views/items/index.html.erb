
	<table class="table">
		<thead>
			<tr>
				<th>listing</th>
				<th>description</th>
				<th>price</th>
				<th>time</th>
			</tr>
		</thead>
		<% @items.each do |item| %>
			<tr class="list_item" url="/items/<%= item.id %>">
				<td><%= item.name %></td>
				<td><%= item.description %></td>
				<td><%= "$ #{item.price}" %></td>
				<td><%= time_ago_in_words(item.created_at)%> ago</td>
			</tr>
		<% end %>
	</table>